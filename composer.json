{
  "name": "firebrandhq/silverstripe-hail",
  "description": "Silverstripe 5 plugin to integrate with Hail (https://get.hail.to)",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "News",
    "Hail",
    "Content"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Firebrand",
      "homepage": "https://firebrand.nz",
      "email": "developers@firebrand.nz"
    }
  ],
  "require": {
    "php": "^8.1",
    "silverstripe/cms": "^5.0",
    "silverstripe/framework": "^5.0",
    "silverstripe/environmentcheck": "^3.0",
    "guzzlehttp/guzzle": "^7",
    "symfony/console": "^6.1",
    "composer-plugin-api": "^2.0"
  },
    "require-dev": {
        "composer/composer": "^2.0"
    },
  "autoload": {
    "psr-4": {
      "Firebrand\\Hail\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist",
      "thirdparty"
    ],
    "branch-alias": {
      "dev-master": "5.0.x-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "suggest": {
    "firebrandhq/silverstripe-hail-elemental": "Add Hail Carousel Element type to Elemental",
    "firebrandhq/silverstripe-hail-opengraph": "Add OpenGraph tags to Hail Pages and Hail Articles"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
